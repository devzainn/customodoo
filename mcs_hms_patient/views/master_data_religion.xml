<?xml version="1.0" encoding="utf-8"?>
<odoo>

    <record id="master_religion_view_form" model="ir.ui.view">
        <field name="name">master_religion_view_form</field>
        <field name="model">master.religion.patient</field>
        <field name="arch" type="xml">
            <form>
                <sheet>
                    <group>
                        <h1><field name="name"/></h1>
                    </group>
                </sheet>
            </form>
        </field>
    </record>

    <record id="master_religion_view_tree" model="ir.ui.view">
        <field name="name">master_religion_view_tree</field>
        <field name="model">master.religion.patient</field>
        <field name="arch" type="xml">
            <tree string="Master Data Agama">
                <field name="name" />
            </tree>
        </field>
    </record>

    <record id="master_religion_action" model="ir.actions.act_window">
        <field name="name">Master Data Agama</field>
        <field name="res_model">master.religion.patient</field>
        <field name="view_mode">tree,form</field>
        <field name="help" type="html">
            <h1>Kosong</h1>
        </field>
    </record>

</odoo>
